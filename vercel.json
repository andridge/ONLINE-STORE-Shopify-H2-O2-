{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "app/**/*.js",
      "use": "@remix-run/dev",
      "config": {
        "maxLambdaSize": "10mb"
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "alias": [
    {
      "source": "/dist/worker/(.*)",
      "destination": "/list/worker/$1"
    }
  ]
}
