{
  "version": 2,
  "builds": [
    {
      "src": "app/server.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "app/styles/tailwind.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app/:1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
